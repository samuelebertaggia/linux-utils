packloadall

autocmd BufWritePre * %s/\s\+$//e

colorscheme dracula

filetype indent plugin on

set autoindent
set backspace=indent,eol,start
set cindent
set expandtab
set hlsearch
set ignorecase
set incsearch
set linebreak
set number
set ruler
set shiftwidth=2
set showmatch
set smartcase
set smartindent
set smarttab
set softtabstop=2
set undolevels=1000
set wildignorecase

syntax on
